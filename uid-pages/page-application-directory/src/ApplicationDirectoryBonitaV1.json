    },
    "applicationData" : {
      "type" : "expression",
      "value" : [
        "return {",
        "    pageId: 0,",
        "    numberOfApplicationsPerPage: 10,",
        "    applicationsInMemory: [],",
        "    applicationsFromAPI: undefined,",
        "    numberOfApplicationsInMemory: 0,",
        "    displayedApplications: [],",
        "    filters: {",
        "        searchValue: \"\"",
        "    }",
        "};"
      ],
      "exposed" : false
    },
    "applicationUrl" : {
      "type" : "url",
      "value" : [
        "../API/living/application{{applicationCtrl.applicationQueryParameters(0)}}"
      ],
      "exposed" : false
    },
    "applicationCtrl" : {
      "type" : "expression",
      "value" : [
        "return {",
        "    applicationQueryParameters: function(pageId) {",
        "        if(!$data.applicationData.numberOfApplicationsPerPage){",
        "            return undefined;",
        "        }",
        "      ",
        "        var applicationsPerPage = $data.applicationData.numberOfApplicationsPerPage;",
        "        if(pageId === 0){",
        "            //On first call we need to load next page",
        "            applicationsPerPage *= 2;",
        "        }",
        "        var queryParameters = \"?c=\" + applicationsPerPage + \"&p=\" + pageId + \"&d=profileId&d=createdBy&d=updatedBy&d=layoutId&f=userId=\" + $data.session.user_id;",
        "        ",
        "        if($data.applicationData.filters.searchValue){",
        "            queryParameters += \"&s=\" +  $data.applicationData.filters.searchValue;",
        "        }",
        "        return queryParameters;",
        "    },",
        "    getIcon: function(icon) {",
        "        return icon === \"\" ? \"../skin/images/logo.png\" : icon;",
        "    },",
        "    sanitizeString: function(stringToSanitize) {",
        "        return stringToSanitize.replaceAll(\"\\\"\", \"&quot;\").replaceAll(\"\\'\", \"&#039;\");",
        "    },",
        "    getApplicationCssClasses: function(application) {",
        "        var cssClasses = \"application-card \";",
        "        ",
        "        if (application.token === $data.currentAppToken) {",
        "            cssClasses += \"application-card--current\";",
        "        }",
        "        ",
        "        return cssClasses;",
        "    }",
        "};"
      ],
      "exposed" : false
    },
    "applicationHandler" : {
      "type" : "expression",
      "value" : [
        "// Applications\r",
        "// display the first page of items on page load or when the filters change\r",
        "if($data.applicationUrl){\r",
        "    // display half the elements loaded\r",
        "    $data.applicationData.displayedApplications = $data.applicationUrl.slice(0, $data.applicationData.numberOfApplicationsPerPage);\r",
        "    // set the next page to be requested to 2 since the first page is in the memory\r",
        "    $data.applicationData.pageId = 2;\r",
        "    $data.applicationData.applicationsInMemory = $data.applicationUrl.slice($data.applicationData.numberOfApplicationsPerPage);\r",
        "    // saving the other half to be displayed later\r",
        "    $data.applicationData.numberOfApplicationsInMemory = $data.applicationData.applicationsInMemory.length;\r",
        "    // set items to undefined as the script is triggered when click on load more items button\r",
        "    // and to prevent this block of code from being executed (prevent resetting the displayedApplications)\r",
        "    $data.applicationUrl = undefined;\r",
        "    $data.applicationData.applicationsFromAPI = undefined;\r",
        "}\r",
        "\r",
        "// add next page of items to the list of next items to display\r",
        "// display the next items from the memory\r",
        "if($data.applicationData.applicationsFromAPI && $data.applicationData.applicationsFromAPI.length){\r",
        "    $data.applicationData.displayedApplications = $data.applicationData.displayedApplications.concat($data.applicationData.applicationsInMemory);\r",
        "    $data.applicationData.pageId++;\r",
        "    $data.applicationData.applicationsInMemory = $data.applicationData.applicationsFromAPI;\r",
        "    $data.applicationData.numberOfApplicationsInMemory = $data.applicationData.applicationsInMemory.length;\r",
        "    $data.applicationData.applicationsFromAPI = undefined;\r",
        "}\r",
        "\r",
        "// when there are no more items in the memory\r",
        "if($data.applicationData.numberOfApplicationsInMemory > 0 && $data.applicationData.applicationsFromAPI && $data.applicationData.applicationsFromAPI.length === 0){\r",
        "     $data.applicationData.displayedApplications = $data.applicationData.displayedApplications.concat($data.applicationData.applicationsInMemory);\r",
        "     $data.applicationData.applicationsInMemory = undefined;\r",
        "     $data.applicationData.applicationsFromAPI = undefined;\r",
        "     $data.applicationData.numberOfApplicationsInMemory = 0;\r",
        "}"
      ],
      "exposed" : false
    },
    "logoUrl" : {
      "type" : "constant",
      "value" : [
        "../theme/images/logo.png"
      ],
      "exposed" : false
    },
    "currentAppToken" : {
      "type" : "urlparameter",
      "value" : [
        "app"
      ],
